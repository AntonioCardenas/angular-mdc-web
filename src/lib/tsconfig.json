{
 "compilerOptions": {
  "target": "es6",
  "module": "commonjs",
  "moduleResolution": "node",
  "outDir": "./../../dist",
  "baseUrl": ".",
  "rootDir": ".",
  "allowSyntheticDefaultImports": true,
  "declaration": true,
  "removeComments": true,
  "sourceMap": true,
  "emitDecoratorMetadata": true,
  "experimentalDecorators": true,
  "inlineSources": true,
  "noImplicitAny": false,
  "suppressImplicitAnyIndexErrors": true,
  "lib": ["es2015", "dom"],
  "typeRoots": ["./../../node_modules/@types"]
 },
 "exclude": [
  "node_modules",
  "dist",
  "src"
 ],
 "files": [
  "./index.ts"
 ],
 "compileOnSave": false,
 "buildOnSave": false
}
