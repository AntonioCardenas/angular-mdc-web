{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": ["es6", "es2015", "dom"],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noStrictGenericChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "sourceMap": true,
    "typeRoots": ["../../node_modules/@types"]
  },
  "files": [
    "./app-module.ts",
    "./main.ts"
  ],
  "exclude": [
    "main-aot.ts"
  ],
  "compileOnSave": false,
  "buildOnSave": false
}
