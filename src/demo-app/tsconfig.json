{
	"compilerOptions": {
		"target": "es6",
		"module": "es6",
		"moduleResolution": "node",
		"outDir": "./../../publish",
		"declaration": false,
		"rootDir": "./../../",
		"removeComments": true,
		"sourceMap": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"inlineSources": true,
		"stripInternal": true,
		"typeRoots": [
			"./../../node_modules/@types"
		]
	}
}
